{
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.12",
    "daisyui": "^4.10.2",
    "tailwindcss": "^3.4.3",
    "prettier": "^3.2.5",
    "prettier-plugin-go-template": "^0.0.15"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-go-template"
    ],
    "overrides": [
      {
        "files": [
          "*.html"
        ],
        "options": {
          "parser": "go-template"
        }
      }
    ]
  }
}
